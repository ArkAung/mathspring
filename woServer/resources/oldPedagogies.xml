        <!--  Builds a subclass of SingleTopicPM which uses JaneEmpathicLC and customized nextProblem and attempt intervention
        handling.   -->
<pedagogy default="true">
<id>41</id>
<active>true</active>
<package>pedModel2</package>
<name>Jane Teach One topic for Assistments</name>
<comment>for Assistments</comment>
<pedagogicalModelClass>SingleTopicPM</pedagogicalModelClass>
<nextProblemInterventionSelectorCombiner class="NextProblemListCombinerIS">
    <nextProblemInterventionSelector>TopicSwitchAskIS</nextProblemInterventionSelector>
    <nextProblemInterventionSelector>ExternalActivityIS</nextProblemInterventionSelector>
</nextProblemInterventionSelectorCombiner>
<attemptInterventionSelectorCombiner class="AttemptListCombinerIS">
    <attemptInterventionSelector>HighlightHintButtonIS</attemptInterventionSelector>
    <attemptInterventionSelector>GuessingAskAffectIS</attemptInterventionSelector>
</attemptInterventionSelectorCombiner>
<learningCompanionClass>JaneEmpathicLC</learningCompanionClass>

</pedagogy>